<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Message" max_length="40" name="x_44552_bi_message" type="collection">
        <element label="Content" max_length="8000" name="content" type="html"/>
        <element label="Expired Date" max_length="40" name="expired_date" type="glide_date_time"/>
        <element label="Groups" max_length="40" name="groups" reference="sys_user_group" type="glide_list"/>
        <element label="Icon" max_length="100" name="icon" type="icon"/>
        <element label="Short Description" max_length="160" name="short_description" type="string"/>
        <element choice="1" default="info" label="Type" max_length="40" name="type" type="choice">
            <choice>
                <element label="Information" sequence="1" value="info"/>
                <element label="Warning" sequence="2" value="warning"/>
                <element label="Danger" sequence="3" value="danger"/>
            </choice>
        </element>
    </element>
</database>
